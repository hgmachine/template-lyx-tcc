%\tocloftpagestyle{fancy}

\fancypagestyle{demais}{
    \fancyhead{} % clear all header fields
    \fancyhead[R]{\footnotesize {\hspace{1.5cm}\textsc{\textbf \thepage}}}
    \fancyhead[L]{\slshape  {\textsc{\leftmark}}}
    \fancyfoot{} % clear all footer fields
    \renewcommand{\headrulewidth}{0.5pt}
    \renewcommand{\footrulewidth}{0pt}
}
\renewcommand{\chaptermark}[1]{
\markboth{\footnotesize  \textsc{
{\color{refFig} \textbf{\chaptername ~\thechapter}}} {\color{refFig}{$\,\,\,\medblacksquare\,\,$} }\ #1}{}}

\fancypagestyle{solo}{
    \fancyhf{}
   \fancyfoot{}
   \fancyhead{} 
    \fancyhead[RO]{}
    \fancyhead[LO]{}
    \cfoot{}
    \fancyfoot[C]{\footnotesize{\textbf \thepage}}
    \renewcommand{\headrulewidth}{0pt}
}
\tocloftpagestyle{empty}







