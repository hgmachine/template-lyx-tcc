%%%%%%%%%%%%%%%%%%%
 \newcommand\boxedSection[3]{{%
%
     \begin{tikzpicture}[inner sep=#3,line width=.4mm, color=refFig!65!black]
         \node[anchor=east,rectangle,draw] at (0,0) (counter) {\color{refFig!70!black}\textbf{#2}};
             \draw (counter.south west)  ++(0.0pt,0.5pt)-- ++($(\linewidth,0) - (2.5pt,0)$);
\node [right of=counter,anchor=west]{\textbf{#1}};
     \end{tikzpicture}
 }}
 \newcommand\boxedSectionB[3]{{%
%
     \begin{tikzpicture}[inner sep=#3,line width=1.5pt]
          %\draw (0,0.4)-- (\linewidth-1.2cm,0.41);
          \node[anchor=east,rectangle,draw,fill=black] at (0,0) (counter) {\color{white}\textbf{#2}};
               \draw (counter.south west)  ++(0.0pt,0.5pt)-- ++($(\linewidth,0) - (2.5pt,0)$);
          \node [right of=counter,anchor=west]{\textsc {#1} };
     \end{tikzpicture}
 }}
 \newcommand\boxedSectionC[3]{{%
%
     \begin{tikzpicture}[inner sep=#3,line width=1pt, color=refFig]
         \node[anchor=east,rectangle,draw,fill=refFig] at (0,0) (counter) {\color{white}\textbf{#2}};
             \draw (counter.south west)  ++(0.0pt,0.5pt)-- ++($(\linewidth,0) - (2.5pt,0)$);
\node [right of=counter,anchor=west]{\textbf{#1}};
     \end{tikzpicture}
 }}
\newcommand\boxedsection[1]{\boxedSectionB{#1}{\thesection}{1.5mm}}
\newcommand\boxedsubsection[1]{\boxedSection{#1}{\thesubsection}{1.5mm}}
\newcommand\boxedsubsubsection[1]{\boxedSectionC{#1}{\thesubsubsection}{1.5mm}}

 \titleformat{\section}[hang]%
     {\bfseries}%
     {}%
     {0em}%
     {\filright\boxedsection}%

 \titleformat{\subsection}[hang]%
     {}%
     {}%
     {.0em}%
     {\filright\boxedsubsection}%

 \titleformat{\subsubsection}[hang]%
     {}%
     {}%
     {.0em}%
     {\filright\boxedsubsubsection}%



